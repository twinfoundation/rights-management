{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"description": "Auto-Generated Release Please configuration for minor",
	"pull-request-title-pattern": "chore: minor release prepared",
	"pull-request-header": "chore: minor release prepared",
	"pull-request-footer": "This PR was generated by the prepare-release GHA",
	"release-type": "node",
	"versioning": "always-bump-minor",
	"packages": {
		"packages/rights-management-models": {
			"package-name": "rights-management-models",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		},
		"packages/rights-management-pap-entity-storage": {
			"package-name": "rights-management-pap-entity-storage",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		},
		"packages/rights-management-service": {
			"package-name": "rights-management-service",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		},
		"packages/rights-management-rest-client": {
			"package-name": "rights-management-rest-client",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		}
	},
	"plugins": [
		"node-workspace",
		{
			"type": "linked-versions",
			"groupName": "repo",
			"components": [
				"rights-management-models",
				"rights-management-pap-entity-storage",
				"rights-management-service",
				"rights-management-rest-client"
			]
		}
	]
}
